{
  "name": "qbjc",
  "version": "0.1.0",
  "description": "QBasic to JavaScript compiler",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/jichu4n/qbjc.git"
  },
  "author": "Chuan Ji <chuan@jichu4n.com>",
  "license": "Apache-2.0",
  "bugs": {
    "url": "https://github.com/jichu4n/qbjc/issues"
  },
  "homepage": "https://github.com/jichu4n/qbjc#readme",
  "bin": "./dist/index.js",
  "scripts": {
    "build:grammar": "cd ./src/parser && nearleyc grammar.ne -o grammar.ts",
    "build:nodeRuntimeBundle": "node ./dist/tools/build-runtime-bundle.js ./dist/runtime/node-runtime.js ./dist/runtime/node-runtime-bundle.js",
    "build:tsc": "tsc && chmod +x ./dist/index.js",
    "build": "npm run build:grammar && npm run build:tsc && npm run build:nodeRuntimeBundle",
    "test": "npm run build && jest"
  },
  "devDependencies": {
    "@types/ansi-escapes": "^4.0.0",
    "@types/ansi-styles": "^3.2.1",
    "@types/fs-extra": "^9.0.5",
    "@types/jest": "^26.0.19",
    "@types/lodash": "^4.14.165",
    "@types/moo": "^0.5.3",
    "@types/nearley": "^2.11.1",
    "@types/node": "^14.14.10",
    "@types/require-from-string": "^1.2.0",
    "jest": "^26.6.3",
    "node-ansiparser": "^2.2.0",
    "node-ansiterminal": "^0.2.1-beta",
    "strip-ansi": "^6.0.0",
    "typescript": "^4.1.2"
  },
  "dependencies": {
    "@vercel/ncc": "^0.26.1",
    "ansi-escapes": "^4.3.1",
    "ansi-styles": "^5.0.0",
    "commander": "^6.2.1",
    "fs-extra": "^9.0.1",
    "legacy-encoding": "^3.0.0",
    "lodash": "^4.17.20",
    "moo": "^0.5.1",
    "nearley": "^2.20.1",
    "require-from-string": "^2.0.2",
    "source-map": "^0.7.3",
    "terser": "^5.5.1"
  }
}
